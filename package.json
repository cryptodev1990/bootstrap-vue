{
  "name": "bootstrap-vue",
  "version": "0.10.0-alpha.2",
  "description": "Bootstrap 4 Components for Vue.js 2",
  "main": "dist/bootstrap-vue.common.js",
  "license": "MIT",
  "homepage": "https://bootstrap-vue.github.io",
  "author": {
    "name": "Pooya Parsa",
    "email": "pooya@pi0.ir",
    "url": "https://github.com/pi0"
  },
  "bugs": {
    "url": "https://github.com/bootstrap-vue/bootstrap-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bootstrap-vue/bootstrap-vue.git"
  },
  "scripts": {
    "build": "yarn build-node && yarn build-web",
    "build-node": "cross-env NODE_ENV=production webpack --config scripts/webpack.common.js --env production --display-error-details",
    "build-web": "cross-env NODE_ENV=production  webpack --config scripts/webpack.js --env production --display-error-details",
    "docs-dev": "node node_modules/nuxt/bin/nuxt dev",
    "docs-gen": "node node_modules/nuxt/bin/nuxt generate",
    "docs-deploy": "cd scripts && ./gh-pages",
    "test": "xo"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-",
    "tether": "latest",
    "vue": "^2.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "codemirror": "^5.24.2",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.2",
    "eslint-plugin-html": "^2.0.0",
    "highlightjs": "^9.8.0",
    "lodash": "latest",
    "node-sass": "^4.5.0",
    "nuxt": "^0.9.9",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^6.0.3",
    "vue-analytics": "^3.0.0",
    "vue-loader": "^11.1.4",
    "vue-server-renderer": "^2.2.3",
    "vue-style-loader": "^2.0.2",
    "vue-template-compiler": "^2.2.3",
    "webpack": "^2.2.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": 4,
    "esnext": true,
    "plugins": [
      "html"
    ],
    "ignores": [
      "docs/plugins/ga.js"
    ],
    "extensions": [
      "vue"
    ],
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "camelcase": 0
    }
  },
  "keywords": [
    "Bootstrap",
    "Bootstrap 4",
    "Web",
    "Components",
    "Polymer",
    "Vue",
    "VueJS",
    "Vue2",
    "WebComponents",
    "jquery",
    "vue-strap",
    "vueify",
    "vuestrap",
    "CSS",
    "Flexbox",
    "Alert",
    "Breadcrumb",
    "Button",
    "Checkbox",
    "Radio",
    "Card",
    "Carousel",
    "Slider",
    "Collapse",
    "Dropdown",
    "Select",
    "Option",
    "Input",
    "Jumbotron",
    "List",
    "Nav",
    "Modal",
    "Navbar",
    "Pagination",
    "Popover",
    "Progress",
    "Tab",
    "Table",
    "Tag",
    "Tooltip",
    "Thether"
  ]
}
